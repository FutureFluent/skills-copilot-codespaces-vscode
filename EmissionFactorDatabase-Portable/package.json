{
  "name": "emission-factor-database",
  "version": "2.0.0",
  "description": "Portable emission factor database with 3-tier matching system based on Exiobase 3.9.6",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "import": "tsx src/import/import-full.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "emission-factors",
    "exiobase",
    "carbon-accounting",
    "scope3",
    "nace",
    "sustainability",
    "climate",
    "co2e"
  ],
  "author": "Your Name",
  "license": "CC-BY-SA-4.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/emission-factor-database"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "csv-parse": "^5.5.6"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2"
  },
  "peerDependencies": {
    "pg": "^8.11.0"
  },
  "peerDependenciesMeta": {
    "pg": {
      "optional": true
    }
  },
  "files": [
    "dist",
    "database",
    "data",
    "docs",
    "README.md",
    "LICENSE.md"
  ]
}
